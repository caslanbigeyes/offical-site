{
  "name": "ZEUS",
  "version": "0.1.0",
  "private": true,
  "author": "alex xu",
  "description": "An enterprise-grade middle and back office solution based on ANTD and NEXTJS",
  "keywords": [
    "ant",
    "next.js",
    "components",
    "admin stystem",
    "frontend",
    "react",
    "react-component"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "build:local": "APP_ENV=local next build",
    "build:dev": "APP_ENV=dev next build",
    "build:prod": "APP_ENV=prod next build",
    "deploy:local": "npm run build && pm2 start pm2.config.js --env local",
    "deploy:dev": "npm build:dev && pm2 start pm2.config.js --env dev",
    "deploy:prod": "npm start pm2.config.js --env prod"
  },
  "dependencies": {
    "@ant-design/icons": "^4.8.2",
    "@ant-design/nextjs-registry": "^1.0.0",
    "antd": "^5.15.2",
    "axios": "^1.6.8",
    "babel-plugin-import": "^1.13.8",
    "classnames": "^2.5.1",
    "copy-to-clipboard": "^3.3.3",
    "crypto": "^1.0.1",
    "next": "14.1.3",
    "next-intl": "^3.9.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/sortablejs": "^1.15.8",
    "autoprefixer": "^10.4.19",
    "eslint": "^8",
    "eslint-config-next": "14.1.3",
    "next-plugin-antd-less": "^1.8.0",
    "postcss": "^8.4.38",
    "typescript": "^5"
  }
}
